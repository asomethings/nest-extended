{
  "name": "@nestjs-extended/monorepo",
  "version": "0.0.0",
  "private": true,
  "description": "Nest.js extensions",
  "type": "module",
  "author": "Ewan Roh (asomethings@kakao.com)",
  "types": "./dist/index.d.ts",
  "exports": "./dist/index.js",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "prerelease": "rimraf  packages/*/dist && pnpm lint && pnpm rollup",
    "rollup": "pnpm -r rollup",
    "test": "pnpm -r test",
    "coverage": "pnpm -r coverage",
    "lint": "pnpm -r lint",
    "benchmark": "node --no-warnings --loader ts-node/esm --experimental-specifier-resolution=node benchmarks/run.ts",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "release": "changeset publish",
    "changeset-rimraf": "rimraf packages/*/CHANGELOG.md"
  },
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@commitlint/cli": "17.8.1",
    "@commitlint/config-conventional": "17.8.1",
    "@rollup/plugin-typescript": "8.3.3",
    "@types/autocannon": "7.6.1",
    "@types/cors": "2.8.12",
    "@types/mustache-express": "1.2.2",
    "@types/node": "16.11.43",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "autocannon": "7.9.0",
    "ava": "4.3.0",
    "c8": "7.11.3",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.1",
    "prettier": "2.7.1",
    "prettier-plugin-organize-imports": "3.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rollup": "2.76.0",
    "rollup-plugin-terser": "7.0.2",
    "rxjs": "7.5.5",
    "source-map-support": "0.5.21",
    "ts-node": "10.8.2",
    "ts-toolbelt": "9.6.0",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@changesets/cli": "2.17.0"
  }
}